{
  "name": "self-editing-discord-bot",
  "version": "1.0.0",
  "description": "Revolutionary self-editing Discord bot with AI-powered autonomous improvement capabilities",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "npm run build && node dist/index.js",
    "build": "esbuild src/index.ts --bundle --platform=node --format=esm --outdir=dist --external:discord.js --external:@anthropic-ai/sdk --external:express --external:dotenv --external:redis --external:chokidar --external:glob",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t discord-bot .",
    "docker:run": "docker run -p 8080:8080 discord-bot",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "keywords": [
    "discord",
    "bot",
    "self-editing",
    "ai",
    "autonomous",
    "typescript",
    "postgresql",
    "redis",
    "vector-database",
    "docker",
    "kubernetes"
  ],
  "author": "Discord Bot Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/self-editing-discord-bot.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.9",
    "@ai-sdk/openai": "^3.0.7",
    "@anthropic-ai/sdk": "^0.71.2",
    "@discordjs/core": "^2.4.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-jaeger": "^1.17.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.45.0",
    "@opentelemetry/exporter-zipkin": "^1.17.0",
    "@opentelemetry/instrumentation": "^0.45.0",
    "@opentelemetry/instrumentation-http": "^0.45.0",
    "@opentelemetry/instrumentation-pg": "^0.36.0",
    "@opentelemetry/instrumentation-redis": "^0.34.0",
    "@opentelemetry/resources": "^1.17.0",
    "@opentelemetry/sdk-trace-base": "^1.17.0",
    "@opentelemetry/sdk-trace-node": "^1.17.0",
    "@opentelemetry/semantic-conventions": "^1.17.0",
    "@qdrant/js-client-rest": "^1.16.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "ai": "^6.0.19",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.0.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.0.0",
    "express-winston": "^4.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.0",
    "jest": "^29.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.2",
    "openai": "^4.20.2",
    "pg": "^8.8.0",
    "pinecone-client": "^1.1.0",
    "prettier": "^3.0.0",
    "prom-client": "^15.1.3",
    "redis": "^4.6.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "uuid": "^9.0.0",
    "weaviate-client": "^3.10.0",
    "winston": "^3.8.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.0",
    "concurrently": "^8.0.0",
    "esbuild": "^0.24.0",
    "nodemon": "^3.0.0",
    "p-queue": "^8.0.1",
    "supertest": "^6.0.0",
    "ts-jest": "^29.4.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/__tests__/**",
      "!src/**/*.test.ts",
      "!src/**/*.spec.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/src/scripts/"
    ]
  }
}
